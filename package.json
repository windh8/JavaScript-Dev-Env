  "license": "MIT",
  "bugs": {
    "url": "https://github.com/windh8/JavaScript-Dev-Env/issues"
  },
  "homepage": "https://github.com/windh8/JavaScript-Dev-Env#readme",
  "dependencies": {
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "ajv": "^6.2.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.3.1",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-watch": "^3.1.3",
    "express": "^4.16.2",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.6.2",
    "json-schema-faker": "^0.3.6",
    "json-server": "^0.12.1",
    "localtunnel": "^1.8.3",
    "mocha": "^5.0.1",
    "nock": "^9.2.1",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.2.1",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.3",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.21.0",
